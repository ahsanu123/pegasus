<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
    <authentication mode="Forms">
      <forms cookieless="AutoDetect" defaultUrl="default.aspx" loginUrl="login.aspx" enableCrossAppRedirects="false" name="security" protection="All" timeout="60" slidingExpiration="true"/>
    </authentication>
    <pages controlRenderingCompatibilityVersion="4.0" clientIDMode="AutoID"/>
  </system.web>
  <location path="default.aspx">
    <system.web>
      <authorization>
        <deny users="?"/>
      </authorization>
    </system.web>
  </location>
</configuration>
